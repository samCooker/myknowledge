<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.myknowledge769957" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>myknowledge</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="you@example.com" href="http://example.com.com/">
      Your Name Here
    </author>
  <content src="index.html"/>
  <access origin="bdapp://map/marker"/>
  <access origin="bdapp://map/direction"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <icon src="resources/android/icon/tr-mdpi-icon.png" density="ldpi"/>
    <icon src="resources/android/icon/tr-mdpi-icon.png" density="mdpi"/>
    <icon src="resources/android/icon/tr-hdpi-icon.png" density="hdpi"/>
    <icon src="resources/android/icon/tr-xhdpi-icon.png" density="xhdpi"/>
    <icon src="resources/android/icon/tr-xhdpi-icon.png" density="xxhdpi"/>
    <splash src="resources/android/splash/tr-land-mdpi-screen.png" density="land-ldpi"/>
    <splash src="resources/android/splash/tr-land-mdpi-screen.png" density="land-mdpi"/>
    <splash src="resources/android/splash/tr-land-hdpi-screen.png" density="land-hdpi"/>
    <splash src="resources/android/splash/tr-land-xhdpi-screen.png" density="land-xhdpi"/>
    <splash src="resources/android/splash/tr-land-xhdpi-screen.png" density="land-xxhdpi"/>
    <splash src="resources/android/splash/tr-port-mdpi-screen.png" density="port-ldpi"/>
    <splash src="resources/android/splash/tr-port-mdpi-screen.png" density="port-mdpi"/>
    <splash src="resources/android/splash/tr-port-hdpi-screen.png" density="port-hdpi"/>
    <splash src="resources/android/splash/tr-port-xhdpi-screen.png" density="port-xhdpi"/>
    <splash src="resources/android/splash/tr-port-xhdpi-screen.png" density="port-xxhdpi"/>
  </platform>
</widget>
